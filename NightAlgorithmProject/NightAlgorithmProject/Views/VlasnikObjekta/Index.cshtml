@model IEnumerable<NightAlgorithm.Models.VlasnikObjekta>

@{
    ViewData["Title"] = "Vlasnik";
}

<h1>Informacije o vlasniku</h1>

<p>
    <a asp-action="Create">Dodaj novog vlasnika</a>
</p>
<table style="color: #ffcd00;"class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.korisničkoIme)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lozinka)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.korisničkoIme)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.lozinka)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
